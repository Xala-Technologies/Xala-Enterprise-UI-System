{
  "id": "enterprise-light",
  "name": "Enterprise Light",
  "description": "Professional light theme for enterprise applications with corporate standards",
  "version": "1.0.0",
  "category": "ENTERPRISE",
  "mode": "LIGHT",
  "accessibility": {
    "level": "WCAG_AA",
    "highContrast": false,
    "reducedMotion": false,
    "focusIndicators": true,
    "colorBlindFriendly": true,
    "screenReaderOptimized": false
  },
  "colors": {
    "primary": {
      "50": "#f8fafc",
      "100": "#f1f5f9",
      "200": "#e2e8f0",
      "300": "#cbd5e1",
      "400": "#94a3b8",
      "500": "#64748b",
      "600": "#475569",
      "700": "#334155",
      "800": "#1e293b",
      "900": "#0f172a",
      "950": "#020617"
    },
    "secondary": {
      "50": "#fafaf9",
      "100": "#f5f5f4",
      "200": "#e7e5e4",
      "300": "#d6d3d1",
      "400": "#a8a29e",
      "500": "#78716c",
      "600": "#57534e",
      "700": "#44403c",
      "800": "#292524",
      "900": "#1c1917",
      "950": "#0c0a09"
    },
    "background": {
      "default": "#ffffff",
      "paper": "#f8fafc",
      "elevated": "#f1f5f9"
    },
    "text": {
      "primary": "#0f172a",
      "secondary": "#475569",
      "muted": "#94a3b8"
    },
    "border": {
      "default": "#e2e8f0",
      "muted": "#f1f5f9"
    },
    "status": {
      "success": "#059669",
      "warning": "#d97706",
      "error": "#dc2626",
      "info": "#0284c7"
    }
  },
  "typography": {
    "fontFamily": {
      "sans": ["IBM Plex Sans", "system-ui", "sans-serif"],
      "serif": ["IBM Plex Serif", "serif"],
      "mono": ["IBM Plex Mono", "monospace"]
    },
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem"
    },
    "fontWeight": {
      "light": 300,
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    },
    "lineHeight": {
      "tight": 1.25,
      "normal": 1.5,
      "relaxed": 1.75
    }
  },
  "spacing": {
    "0": "0",
    "1": "0.25rem",
    "2": "0.5rem",
    "3": "0.75rem",
    "4": "1rem",
    "6": "1.5rem",
    "8": "2rem",
    "12": "3rem",
    "16": "4rem",
    "24": "6rem"
  },
  "branding": {
    "logo": {
      "primary": "/assets/logos/enterprise-logo-light.svg",
      "secondary": "/assets/logos/enterprise-logo-alt-light.svg"
    },
    "corporateColors": {
      "main": "#64748b",
      "accent": "#78716c"
    }
  },
  "compliance": {
    "standards": ["GDPR", "SOC2", "ISO27001"],
    "dataClassification": "CONFIDENTIAL",
    "requiresApproval": true,
    "auditLogging": true
  },
  "performance": {
    "lazyLoading": true,
    "cacheStrategy": "moderate",
    "bundleOptimization": true
  },
  "responsive": {
    "breakpoints": {
      "mobile": "320px",
      "tablet": "768px",
      "desktop": "1024px",
      "wide": "1440px"
    }
  }
}

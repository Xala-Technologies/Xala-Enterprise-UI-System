{
  "shadows": {
    "elevation": {
      "none": "none",
      "xs": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
      "sm": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)",
      "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04)",
      "2xl": "0 25px 50px -12px rgba(0, 0, 0, 0.25)",
      "inner": "inset 0 2px 4px 0 rgba(0, 0, 0, 0.06)"
    },
    "colored": {
      "outline": {
        "none": "0 0 0 0px transparent",
        "sm": "0 0 0 1px currentColor",
        "md": "0 0 0 2px currentColor",
        "lg": "0 0 0 3px currentColor"
      },
      "ring": {
        "none": "0 0 0 0px transparent",
        "sm": "0 0 0 1px var(--color-primary)",
        "md": "0 0 0 2px var(--color-primary)",
        "lg": "0 0 0 3px var(--color-primary)"
      },
      "focus": {
        "default": "0 0 0 2px var(--color-primary)",
        "ring": "0 0 0 2px var(--color-background), 0 0 0 4px var(--color-primary)"
      }
    },
    "semantic": {
      "button": {
        "default": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
        "hover": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
        "focus": "0 0 0 2px var(--color-background), 0 0 0 4px var(--color-primary)"
      },
      "card": {
        "default": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06)",
        "hover": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)"
      },
      "input": {
        "default": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
        "focus": "0 0 0 2px var(--color-primary)"
      },
      "popover": {
        "default": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)"
      },
      "dialog": {
        "default": "0 25px 50px -12px rgba(0, 0, 0, 0.25)"
      },
      "toast": {
        "default": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)"
      },
      "dropdown": {
        "default": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)"
      }
    }
  }
}
{
  "name": "@xala-technologies/ui-system",
  "version": "3.1.0",
  "description": "Enterprise-compliant UI system with Norwegian accessibility standards, design tokens, and zero inline styles following @xala-technologies/enterprise-standards v6.0.2",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./localization": {
      "types": "./dist/localization/index.d.ts",
      "import": "./dist/localization/index.js",
      "require": "./dist/localization/index.js"
    },
    "./rtl": {
      "types": "./dist/rtl/index.d.ts",
      "import": "./dist/rtl/index.js",
      "require": "./dist/rtl/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.js"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "import": "./dist/tokens/index.js",
      "require": "./dist/tokens/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run build:ts && pnpm run fix:imports && pnpm run build:css",
    "build:lib": "pnpm run build:ts && pnpm run fix:imports && pnpm run build:css",
    "build:ts": "tsc --project tsconfig.build.json",
    "fix:imports": "node scripts/fix-imports.js",
    "build:css": "mkdir -p dist && echo '/* Enterprise UI System Tokens */' > dist/tokens.css",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --passWithNoTests --watchAll=false",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:lenient": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run format && npm run lint && npm run type-check && npm run test",
    "validate:comprehensive": "pnpm run validate && pnpm run test:coverage",
    "validate:components": "node scripts/validate-action-feedback-components.js && node scripts/validate-data-display-components.js && node scripts/validate-form-components.js && node scripts/validate-layout-components.js && node scripts/validate-platform-components.js",
    "validate:pasepage": "node scripts/validate-pasepage.js",
    "validate:design-tokens": "node scripts/validate-design-tokens.js",
    "validate:publication": "node scripts/validate-publication.js",
    "validate:all": "pnpm run validate:components && pnpm run validate:pasepage && pnpm run validate:design-tokens && pnpm run validate:publication",
    "enterprise:generate": "npx @xala-technologies/enterprise-standards generate --platform library",
    "enterprise:validate": "npx @xala-technologies/enterprise-standards validate",
    "enterprise:info": "npx @xala-technologies/enterprise-standards info",
    "enterprise:ai": "npx @xala-technologies/enterprise-standards ai --platform library",
    "ci:validate": "pnpm run validate:comprehensive && pnpm run validate:all && pnpm run test:ci && pnpm run enterprise:validate",
    "prepublishOnly": "pnpm run clean && pnpm run build:lib && pnpm run enterprise:validate",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "keywords": [
    "ui-system",
    "norwegian",
    "compliance",
    "wcag",
    "nsm",
    "gdpr",
    "design-tokens",
    "accessibility",
    "pasepage",
    "react",
    "components"
  ],
  "author": "Xala Technologies",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xala-technologies/ui-system.git"
  },
  "homepage": "https://github.com/xala-technologies/ui-system#readme",
  "bugs": {
    "url": "https://github.com/xala-technologies/ui-system/issues"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@xala-technologies/enterprise-standards": "6.2.0",
    "axe-core": "^4.8.3",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^51.3.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "msw": "^2.0.11",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.12",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "directories": {
    "test": "tests"
  },
  "type": "commonjs"
}
